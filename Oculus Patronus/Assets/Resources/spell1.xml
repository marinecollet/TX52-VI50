<spellList>
    <shot><!-- launch a shot-->
        <collider>LEFT</collider>
        <collider>CENTER</collider>
    </shot>
        
    <holohomora><!-- open the door-->
        <collider>TOP</collider>
        <collider>CENTER</collider>
        <collider>BOTTOM</collider>
    </holohomora>

    <lave><!-- move object-->
        <collider>BOTTOM</collider>
        <collider>CENTER</collider>
    </lave>
        
    <protego><!-- protect the player from enemy shot-->
        <collider>LEFT</collider>
        <collider>BOTTOM</collider>
    </protego>
</spellList>